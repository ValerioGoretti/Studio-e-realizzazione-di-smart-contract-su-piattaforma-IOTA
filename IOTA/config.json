{
  "database": {
    "directory": "waspdb"
  },
  "logger": {
    "level": "debug",
    "disableCaller": false,
    "disableStacktrace": false,
    "encoding": "console",
    "outputPaths": [
      "stdout",
      "wasp.log"
    ],
    "disableEvents": true
  },
  "network": {
    "bindAddress": "0.0.0.0",
    "externalAddress": "auto"
  },
  "node": {
    "disablePlugins": [],
    "enablePlugins": []
  },
  "webapi": {
    "bindAddress": "127.0.0.1:9090"
  },
  "peering":{
    "port": 31415,
	"netid": "127.0.0.1:31415"
  },
  "nodeconn": {
    "address": "127.0.0.1:5000"
  },
  "dashboard": {
  	"auth": {
	  "scheme": "basic",
	  "username": "wasp",
	  "password": "wasp"
	},
    "bindAddress": "127.0.0.1:7000"
  }
}
